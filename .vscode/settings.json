{
    "git.autofetch": true,
    "gitdoc.enabled": true,
    "gitdoc.autoCommitDelay": 60000,
    "gitdoc.commitMessageFormat": "ccc, LLL d, kkkk, h:mm a ZZ",
    "flake8.args": [
        "--max-line-length=100"
    ],
    "pylint.args": [
        "C0114"
    ],
    "files.insertFinalNewline": true
}
